// .eslintrc file (no extension)
{
  "extends": [
    "react-app"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "semi": "warn",
    "no-extra-semi": "warn",
    "semi-style": "warn",
    "sort-keys": "error",
    "jsx-quotes": [
      "error",
      "prefer-double"
    ]
  },
  "overrides": [
    {
      "files": [
        "**/*.ts",
        "**/*.tsx"
      ]
    }
  ]
}